{
  "name": "@openhps/openvslam",
  "version": "0.1.0-alpha.0",
  "description": "Open Hybrid Positioning System",
  "author": "Maxim Van de Wynckel",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenHPS/openhps-openvslam.git"
  },
  "bugs": {
    "url": "https://github.com/OpenHPS/openhps-openvslam/issues"
  },
  "homepage": "https://openhps.org",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "lsd:module": true,
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm/index.js",
  "gypfile": true,
  "files": [
    "dist/**/*",
    "src/**/*",
    "build/**/*",
    "cc/**/*",
    "install/**/*",
    "data/**/**",
    "LICENSE"
  ],
  "scripts": {
    "install": "node ./install/install.js",
    "test": "mocha --config test/.mocharc.json",
    "test:docker": "docker-compose run openvslam-cpu /opt/app/docker/docker-entrypoint.sh",
    "test:docker:quick": "docker-compose run openvslam-cpu /opt/app/docker/docker-entrypoint-quick.sh",
    "cover": "nyc mocha --config test/.mocharc.json && nyc report --reporter=cobertura",
    "cover:ci": "nyc mocha --config test/.mocharc.json --reporter mocha-junit-reporter && nyc report --reporter=cobertura",
    "lint": "eslint src/**",
    "clean": "shx rm -rf ./dist && shx rm -rf ./docs/out",
    "build": "npm run clean && npm-run-all --parallel build:ts:*",
    "build:components": "componentsjs-generator",
    "build:quick": "npm-run-all --parallel build:ts:*",
    "build:typedoc": "typedoc --options docs/typedoc.json",
    "build:ts:cjs": "tsc --build ./tsconfig/tsconfig.bundle.cjs.json",
    "build:ts:esm": "tsc --build ./tsconfig/tsconfig.bundle.esm.json",
    "build:ts:esm5": "tsc --build ./tsconfig/tsconfig.bundle.esm5.json",
    "build:ts:types": "tsc --build ./tsconfig/tsconfig.bundle.types.json",
    "deploy:typedoc": "gh-pages -d ./docs/out",
    "bump:development": "standard-version --prerelease alpha -m 'chore(alpha): %s' --skip.changelog",
    "bump:beta": "standard-version --prerelease beta -m 'chore(beta): %s' --skip.changelog",
    "bump:release": "standard-version -m 'chore(release): %s' --skip.changelog"
  },
  "keywords": [
    "openhps",
    "openvslam"
  ],
  "peerDependencies": {
    "@openhps/opencv": ">=0.1.2",
    "@openhps/core": ">=0.3.35",
    "reflect-metadata": ">=0.1.13"
  },
  "dependencies": {
    "@openhps/opencv": "^0.1.2",
    "nan": "^2.15.0",
    "native-node-utils": "^0.2.7",
    "node-addon-api": "^5.0.0",
    "npmlog": "^6.0.2",
    "protobufjs": "^6.11.2",
    "socket.io-client": "v2-latest"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@openhps/core": "^0.3.35",
    "@openhps/video": "^0.1.10",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": ">=8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prettier": ">=4.0.0",
    "husky": "^8.0.1",
    "jenkins-mocha": "^9.0.0",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "reflect-metadata": ">=0.1.13",
    "shx": "^0.3.4",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "nyc": {
    "branches": 0,
    "lines": 20,
    "functions": 17,
    "statements": 20,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
